{
  "name": "ember-cli-swiper",
  "version": "1.0.6",
  "description": "Enables touchfriendly, swipeable, carousel-like slides through nolimits4web/Swiper",
  "keywords": [
    "ember-addon",
    "swiper",
    "touch",
    "carousel"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "prepack": "rollup --config",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "test:all": "ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ember-Swiper/ember-cli-swiper"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "swiper": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-transform-typescript": "^7.20.13",
    "@babel/preset-typescript": "^7.18.6",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/ember__component": "^4.0.12",
    "@types/ember__destroyable": "^4.0.1",
    "@types/ember__object": "^4.0.5",
    "@types/ember__runloop": "^4.0.2",
    "@types/ember__debug": "^4.0.3",
    "@types/ember__polyfills": "^4.0.1",
    "rollup": "^3.12.0",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "app-js": {
      "./components/swiper-container.js": "./dist/_app_/components/swiper-container.js",
      "./components/swiper-slide.js": "./dist/_app_/components/swiper-slide.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ]
}
