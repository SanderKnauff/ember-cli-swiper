{
  "name": "ember-cli-swiper",
  "version": "1.0.6",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "workspaces": [
    "ember-cli-swiper",
    "test-app"
  ],
  "scripts": {
    "build": "pnpm --filter ember-cli-swiper build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-cli-swiper start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "prettier": "^2.8.3",
    "eslint": "^8.33.0"
  }
}
